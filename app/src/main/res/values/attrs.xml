<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ChartView">
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="guide_line_color" format="color" />
        <attr name="guide_line_width" format="dimension" />
        <attr name="content_bg_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MiniChartView">
        <attr name="range_start" format="float" />
        <attr name="range_end" format="float" />
        <attr name="window_stroke_color" format="color" />
        <attr name="window_stroke_width_top_bottom" format="dimension" />
        <attr name="window_stroke_width_left_right" format="dimension" />
        <attr name="fade_bg_color" format="color" />
        <attr name="fade_alpha" format="float" />
        <attr name="chart_line_width" format="dimension" />
    </declare-styleable>

</resources>